<h1>Editing Attachment</h1>

<div class="container">
<%= form_for @attachment, :url=>{:action=>"update"},:html => { :multipart => true } do |form| %>
<%= render partial: 'form' ,locals: {f: form}%>
<% end %>

<%= link_to 'Show', @attachment ,class:'btn btn-info col-xs-6 col-md-6' %>
<%= link_to 'Back', attachments_path ,class:'btn btn-info col-xs-6 col-md-6' %>
    </div>